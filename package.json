{
  "name": "neo-register",
  "version": "1.1.3",
  "description": "Neo 自定义组件注册器（支持 react 和 vue2.0 技术栈），主要用于注册 Neo 自定义组件和自定义组件模型。",
  "keywords": [
    "neo自定义组件注册器",
    "neo自定义组件",
    "neo-editor自定义组件模型"
  ],
  "author": "wibetter",
  "license": "MIT",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "scripts": {
    "linkDebug": "neo linkDebug",
    "build2lib": "neo build2lib",
    "build2esm": "neo build2esm",
    "format": "prettier --write \"src/**/**/*.{js,jsx,ts,tsx,vue,scss,json}\""
  },
  "files": [
    "dist/*",
    "docs/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/**/*.{js,jsx,ts,tsx,vue,scss,json}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xsy-neoui/neo-register.git"
  },
  "bugs": {
    "url": "https://github.com/xsy-neoui/neo-register/issues"
  },
  "dependencies": {
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "antd": "^4.9.4",
    "neo-open-api": "^1.0.11",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^9.1.1",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.15",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "neo-cmp-cli": "^1.10.2",
    "prettier": "^2.0.5"
  },
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 8.0.0"
  }
}
